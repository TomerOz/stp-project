er = require('content-security-policy-builder')\n\n// default-src 'self' default.com; script-src scripts.com; whatever-src something; object-src\nbuilder({\n  directives: {\n    defaultSrc: [\"'self'\", 'default.com'],\n    scriptSrc: 'scripts.com',\n    'whatever-src': 'something',\n    objectSrc: true\n  }\n})\n```\n","maintainers":[{"name":"evanhahn","email":"me@evanhahn.com"}],"time":{"modified":"2019-06-13T18:17:06.720Z","created":"2015-09-10T17:49:57.708Z","0.1.0":"2015-09-10T17:49:57.708Z","0.2.0":"2015-09-18T22:27:21.821Z","1.0.0":"2015-12-18T19:23:24.880Z","1.1.0":"2016-07-31T20:32:57.698Z","2.0.0":"2018-01-23T16:31:49.095Z","2.1.0":"2019-06-13T18:17:04.187Z"},"homepage":"https://github.com/helmetjs/content-security-policy-builder","keywords":["security","content","security","policy","csp","builder"],"repository":{"type":"git","url":"git://github.com/helmetjs/content-security-policy-builder.git"},"author":{"name":"Evan Hahn","email":"me@evanhahn.com","url":"https://evanhahn.com"},"bugs":{"url":"https://github.com/helmetjs/content-security-policy-builder/issues","email":"me@evanhahn.com"},"readmeFilename":"README.md","license":"MIT","users":{"thasmo":true,"knoja4":true},"_etag":"W/\"9cccf408ec780de164cbc14e393c7aa0\"","_lastModified":"Thu, 13 Jun 2019 18:17:07 GMT"}